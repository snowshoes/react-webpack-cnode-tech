{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "classes": true,
      "defaultParams": true,
      "modules": true
    }
  },
  "rules": {
    "semi": 0,
    "space-infix-ops": 0,
    "linebreak-style": "off",
    "quotes": "off",
    "comma-dangle": 0,
    "max-len": 0,
    "object-curly-spacing": 1,
    "arrow-body-style": 0,
    "prettier/prettier": "error",
    "react/jsx-space-before-closing": 0,
    "react/jsx-indent": 0,
    "no-unused-expressions": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/require-default-props": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
